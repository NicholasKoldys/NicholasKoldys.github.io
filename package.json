    "start": "pnpm run --parallel \"open:src\"",
    "open:src": "node build/devServer.js",
    "open:dist": "node build/distServer.js",
    "clean:dist": "rimraf ./dist && mkdir dist",
    "prebuild": "pnpm run --stream --parallel \"clean:dist\"",
    "build": "pnpm exec webpack build --config ./build/webpack.prod.config.js --stats verbose",
    "postbuild": "pnpm run --stream --parallel \"open:dist\"",
    "predevbuild": "pnpm clean:dist",
    "devbuild": "pnpm exec webpack build --config ./build/webpack.dev.config.js --stats verbose"
  },
