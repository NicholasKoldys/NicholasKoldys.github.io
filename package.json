{
  "name": "javascript-development-environment",
  "version": "1.0.0",
  "description": "JavaScript development environment",
  "scripts": {
    "prestart": "node buildScripts/startMessage.mjs",
    "start": "npm-run-all --parallel open:src lint:watch test",
    "open:src": "node buildScripts/srcServer.mjs",
    "mock:userdata": "node buildScripts/generateMockData",
    "lint": "npx eslint webpack.config.* src/* buildScripts/* --color",
    "lint:watch": "npm run lint",
    "css": "npx sass public/style",
    "css:watch": "npm run css -- --watch",
    "test": "npx jasmine --config=./jasmine.json",
    "clean:dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean:dist test lint",
    "build": "node buildScripts/build.mjs",
    "postbuild": "node buildScripts/distServer.mjs"
  },
  "author": "Nicholas Koldys",
  "license": "MIT",
  "devDependencies": {
    "chalk": "4.1.2",
    "compression": "1.7.4",
    "css-loader": "6.8.1",
    "eslint": "^8.42.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-watch": "8.0.0",
    "express": "4.18.2",
    "html-webpack-plugin": "5.5.1",
    "jasmine": "^5.0.2",
    "jsdom": "^22.1.0",
    "mini-css-extract-plugin": "^2.7.6",
    "mini-svg-data-uri": "^1.4.4",
    "npm-run-all": "4.1.5",
    "rimraf": "5.0.1",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "style-loader": "3.3.3",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "6.1.1",
    "workbox-webpack-plugin": "^7.0.0",
    "workbox-window": "^7.0.0"
  }
}
